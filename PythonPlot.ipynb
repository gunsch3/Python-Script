    "import matplotlib.pyplot as plt\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "df = pd.read_csv(\"regrex1.csv\")\n",
    "df.plot(kind='scatter',x='x',y='y')\n",
    "\n",
    "plt.savefig('py_orig.png')\n",
    "\n",
    "m, b = np.polyfit(df.x, df.y, 1)\n",
    "plt.plot(df.x, m*df.x+b)\n",
    "\n",
    "plt.savefig('py_lm.png')"
   ]
